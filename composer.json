{
    "name": "katerinaluuu/prodvigate",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.8",
        "roave/security-advisories": "dev-latest"
    },
    "scripts": {
      "check": [
        "phpcs --standard=PSR12 src/",
        "vendor/bin/phpstan analyse -c phpstan.neon src/ --level 8"
      ]
    }
}
